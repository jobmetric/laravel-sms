{
    "name": "jobmetric/laravel-sms",
    "description": "This package is for sending SMS in different Laravel projects.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "jobmetric",
        "package",
        "sms"
    ],
    "authors": [
        {
            "name": "Majid Mohammadian",
            "email": "majeedmohammadian@gmail.com",
            "homepage": "https://www.linkedin.com/in/majidmohammadian/",
            "role": "Full Stack Developer"
        }
    ],
    "require": {
        "php": ">=8.0.1",
        "laravel/framework": ">=9.19",
        "jobmetric/laravel-package-core": "^1.19.1",
        "jobmetric/domi": "^1.13.1",
        "jobmetric/panelio": "^1.16",
        "jobmetric/laravel-location": "^1.2.2",
        "jobmetric/authio": "^1.0",
        "spatie/laravel-query-builder": "^5.7",
        "kavenegar/php": "^1.2"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "JobMetric\\Sms\\": "src/",
            "JobMetric\\Sms\\Factories\\": "database/factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JobMetric\\Sms\\SmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "support": {
        "issues": "https://github.com/jobmetric/laravel-sms/issues",
        "forum": "https://twitter.com/jobmetric",
        "source": "https://github.com/jobmetric/laravel-sms"
    }
}
